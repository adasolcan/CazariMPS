<!doctype html>
<html lang="en">
<head>
  <title>Cazari</title>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

  <link href="/assets/bootstrap.css" rel="stylesheet" />
  <style>body{padding-top: 60px;}
      </style>
  <link href="/assets/bootstrap-responsive.css" rel="stylesheet" />
  <link href="/assets/bwizard.min.css" rel="stylesheet" />
  <link href="/assets/jasny-bootstrap.css" rel="stylesheet" />
  <link href="/assets/jquery-ui-1.10.1.custom.min.css" rel="stylesheet"/>
  </head>
<body>

 <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="brand" href="index.html">Cazari</a>
          <div class="nav-collapse collapse nav-bar-inverse-collapse">
            <ul class="nav pull-right">
		    <li><a href="/metodologie">Metodologie</a></li>
                    <li><a href="/auth/autentificare">Autentificare </a></li>
                    <li><a href="/prezentare_camine">Prezentare cămine </a></li>

            </ul>
          </div>
        </div>
      </div>
    </div>


 <div class="well">
  <div class="span12">

<h1>Lista căminelor</h1>

<table class="table table-striped" style="margin-top:10px; float:left;">
  <tr>
    <th>Category</th>
    <th>Name</th>
    <th>Address</th>
    <th>Description</th>
    <th>Administrator</th>
    <th>Telephone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dormitories.each do |dormitory| %>
  <tr>
    <td><%= dormitory.category_id %></td>
    <td><%= dormitory.name %></td>
    <td><%= dormitory.address %></td>
    <td><%= dormitory.description %></td>
    <td><%= dormitory.administrator %></td>
    <td><%= dormitory.telephone %></td>
    <td><%= link_to 'Show', dormitory %></td>
    <td><%= link_to 'Edit', edit_dormitory_path(dormitory) %></td>
    <td><%= link_to 'Destroy', dormitory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
 </div>
  <div class="clearfix"></div>
    </div>



<%= link_to 'New Dormitory', new_dormitory_path %>

<div class="bottom-logo">
    <img src="/assets/logo.png" width="150px" height="150px"/>
    </div>


  <script src="/assets/jquery.min.js" type="text/javascript"></script>
  <script src="/assets/bootstrap.js" type="text/javascript"></script>
  <script src="/assets/jasny-bootstrap.js" type="text/javascript"></script>
  <script src="/assets/bwizard.js" type="text/javascript"></script>
  <script src="/assets/jquery-ui-1.10.1.custom.min.js" type="text/javascript"></script>
</body>
</html>
